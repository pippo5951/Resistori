<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="giubotta.gbsoft.resistori">

    <application
        android:allowBackup="true"
        android:icon="@drawable/res_ico"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CalcolaActivity"
            android:label="@string/title_activity_calcola"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TabelleActivity"
            android:label="@string/title_activity_tabelle"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>